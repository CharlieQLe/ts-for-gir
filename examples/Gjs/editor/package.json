{
  "name": "gjs-ts-editor-example",
  "version": "0.0.1",
  "description": "",
  "main": "main.ts",
  "scripts": {
    "build": "npm run build:types && npm run build:wp",
    "build:wp": "npm run clear:ts && webpack --env production",
    "build:types": "../../../lib/start.js generate Gtk-3.0 GtkSource-3.0 -e gjs",
    "clear:types": "rm -rf ./@types",
    "clear:ts": "rm -rf ./dist",
    "clear": "npm run clear:ts && npm run clear:types",
    "start": "npm run build && npm run start:gjs",
    "start:gjs": "gjs dist/main.js",
    "watch": "npm run build -- --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
