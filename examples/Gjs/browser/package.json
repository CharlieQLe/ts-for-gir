    "build:types": "../../../bin/run generate Gtk-3.0 Pango-1.0 WebKit2-4.0 -e gjs",
    "clear:types": "rm -rf ./@types",
    "clear:ts": "rm -rf ./dist",
    "clear": "npm run clear:ts && npm run build:types",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
