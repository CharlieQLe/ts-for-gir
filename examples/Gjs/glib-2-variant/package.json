{
  "name": "ts-for-gir-glib-2-variant-example",
  "version": "1.0.0",
  "description": "ts-for-gir GVariant example based on https://gjs.guide/guides/glib/gvariant.html",
  "main": "index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:types": "npm run ts-for-gir -- generate GLib-2.0 Gio-2.0 -e gjs --useNamespace -t esm",
    "build:app": "node esbuild.js",
    "build": "npm run build:types && npm run build:app",
    "start:app": "gjs -m dist/index.js",
    "debug:app": "GTK_DEBUG=interactive npm run start:app",
    "start": "npm run build && npm run start:app",
    "validate": "npm run validate:types && npm run validate:app",
    "validate:types": "tsc ./@types/Gjs/*.ts --noEmit",
    "validate:app": "tsc --noEmit",
    "ts-for-gir": "../../../lib/start.js"
  },
  "author": "Pascal Garber <pascal@artandcode.studio>",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.14.48"
  }
}
