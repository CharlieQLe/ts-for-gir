{
  "name": "node-ts-cli-example",
  "version": "0.0.1",
  "description": "",
  "main": "main.ts",
  "scripts": {
    "build": "npm run build:types && npm run build:app",
    "build:app": "npm run clear:ts && webpack --env production",
    "build:types": "../../../lib/start.js generate GLib-2.0 -e node",
    "clear:types": "rm -rf ./@types",
    "clear:ts": "rm -rf ./dist",
    "clear": "npm run clear:ts && npm run clear:types",
    "start": "npm run clear && npm run build && npm run start:app",
    "start:app": "node dist/main.js",
    "watch": "npm run build:app -- --watch",
    "lint": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "raw-loader": "^4.0.2",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "node-gtk": "^0.10.0"
  }
}
