{
  "name": "ts-for-gir",
  "version": "3.0.0-beta.1",
  "description": "Typescript .d.ts generator from GIR for gjs and node-gtk",
  "module": "lib/index.js",
  "type": "module",
  "bin": "./lib/start.js",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "yarn workspace @ts-for-gir/cli run start",
    "update:submodules": "git submodule update --init --recursive && git submodule foreach git pull origin master",
    "update:packages": "ncu -u --deep && yarn install --workspaces",
    "update": "yarn run update:submodules && yarn run update:packages",
    "test": "yarn run regress && yarn run test:ava && yarn run test:girs:all && yarn run build && yarn run validate:examples:app && yarn run build:examples:all && yarn run start:cli-examples:all",
    "test:ava": "yarn run build:ts && yarn run build:test:ava && ava tmp/test/ava/test.js",
    "test:girs:all": "yarn run build:types:all && yarn run validate:types:all",
    "test:girs:gjs": "yarn run build:types:gjs && yarn run validate:types:gjs",
    "test:girs:node": "yarn run build:types:node && yarn run validate:types:node",
    "test:girs:vda1": "yarn run build:types:vda1 && yarn run validate:types:vda1",
    "test:girs:gtk2": "yarn run build:types:gtk2 && yarn run validate:types:gtk2",
    "test:girs:gtk3": "yarn run build:types:gtk3 && yarn run validate:types:gtk3",
    "test:girs:gtk4": "yarn run build:types:gtk4 && yarn run validate:types:gtk4",
    "test:girs:modemmanager1": "yarn run build:types:modemmanager1 && yarn run validate:types:modemmanager1",
    "test:girs:timezonemap1": "yarn run build:types:timezonemap1 && yarn run validate:types:timezonemap1",
    "test:girs:rygelcore2": "yarn run build:types:rygelcore2 && yarn run validate:types:rygelcore2",
    "regress": "yarn run build:ts && yarn run start generate --configName='../../.ts-for-gir.regress.rc.js' && yarn run validate",
    "build": "yarn run build --workspace=@ts-for-gir/cli",
    "build:ts": "yarn run build:ts --workspace=@ts-for-gir/cli",
    "build:examples:all": "yarn run build && yarn run build --workspaces",
    "build:examples:gjs": "yarn run build --workspace=ts-for-gir-gtk-3-browser-example --workspace=ts-for-gir-gtk-3-builder-example --workspace=ts-for-gir-gtk-3-editor-example --workspace=ts-for-gir-gtk-3-hello-example --workspace=ts-for-gir-glib-2-spawn-command-example",
    "build:examples:node": "yarn run build --workspace=node-ts-gtk-3-browser-example --workspace=node-ts-gtk-3-builder-example --workspace=node-ts-gtk-3-editor-example --workspace=node-ts-gtk-3-hello-example --workspace=node-ts-gtk-3-builder-auto-connect-signals-example --workspace=node-ts-glib-2-spawn-command-example",
    "start:cli-examples:all": "yarn run start:cli-examples:gjs && yarn run start:cli-examples:node",
    "start:cli-examples:gjs": "yarn run start --workspace=ts-for-gir-glib-2-spawn-command-example",
    "start:cli-examples:node": "yarn run start --workspace=node-ts-glib-2-spawn-command-example",
    "build:types:all": "yarn run start generate --configName='../../.ts-for-gir.all.rc.js'",
    "build:types:vda1": "yarn run start generate --configName='../../.ts-for-gir.vda1.rc.js'",
    "build:types:gtk2": "yarn run start generate --configName='../../.ts-for-gir.gtk2.rc.js'",
    "build:types:gtk3": "yarn run start generate --configName='../../.ts-for-gir.gtk3.rc.js'",
    "build:types:gtk4": "yarn run start generate --configName='../../.ts-for-gir.gtk4.rc.js'",
    "build:types:modemmanager1": "yarn run start generate --configName='../../.ts-for-gir.modemmanager1.rc.js'",
    "build:types:timezonemap1": "yarn run start generate --configName='../../.ts-for-gir.timezonemap1.rc.js'",
    "build:types:rygelcore2": "yarn run start generate --configName='../../.ts-for-gir.rygelcore2.rc.js'",
    "build:types:gcalc": "yarn run start generate --configName='../../.ts-for-gir.gcalc.rc.js'",
    "build:types:gjs": "yarn run build:types:all --environments=gjs",
    "build:types:gjs:vda1": "yarn run build:types:vda1 --environments=gjs",
    "build:types:gjs:gtk2": "yarn run build:types:gtk2 --environments=gjs",
    "build:types:gjs:gtk3": "yarn run build:types:gtk3 --environments=gjs",
    "build:types:gjs:gtk4": "yarn run build:types:gtk4 --environments=gjs",
    "build:types:gjs:modemmanager1": "yarn run build:types:modemmanager1 --environments=gjs",
    "build:types:gjs:timezonemap1": "yarn run build:types:timezonemap1 --environments=gjs",
    "build:types:gjs:rygelcore2": "yarn run build:types:rygelcore2 --environments=gjs",
    "build:types:gjs:gcalc": "yarn run build:types:gcalc --environments=gjs",
    "build:types:node": "yarn run clear:types:node && yarn run build:types:all --environments=node",
    "build:types:node:vda1": "yarn run build:types:vda1 --environments=node",
    "build:types:node:gtk2": "yarn run build:types:gtk2 --environments=node",
    "build:types:node:gtk3": "yarn run build:types:gtk3 --environments=node",
    "build:types:node:gtk4": "yarn run build:types:gtk4 --environments=node",
    "build:types:node:modemmanager1": "yarn run build:types:modemmanager1 --environments=node",
    "build:types:node:timezonemap1": "yarn run build:types:timezonemap1 --environments=node",
    "build:types:node:rygelcore2": "yarn run build:types:rygelcore2 --environments=node",
    "build:types:node:gcalc": "yarn run build:types:gcalc --environments=node",
    "build:test:ava": "tsc -p tsconfig.ava.json",
    "validate": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit",
    "validate:types:all": "yarn run validate:types:gjs && yarn run validate:types:node",
    "validate:types:gjs": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit ./@types/Gjs/*.ts",
    "validate:types:node": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit ./@types/node-gtk/*.ts",
    "validate:types:vda1": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit ./@types/*/Vda-1.d.ts",
    "validate:types:gtk2": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit ./@types/*/Gtk-2.0.d.ts",
    "validate:types:gtk3": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit ./@types/*/Gtk-3.0.d.ts",
    "validate:types:gtk4": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit ./@types/*/Gtk-4.0.d.ts",
    "validate:types:modemmanager1": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit ./@types/*/ModemManager-1.0.d.ts",
    "validate:types:timezonemap1": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit ./@types/*/TimezoneMap-1.0.d.ts",
    "validate:types:rygelcore2": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit ./@types/*/RygelCore-2.6.d.ts",
    "validate:types:gcalc": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit ./@types/*/GCalc-*.d.ts",
    "validate:examples": "yarn run build --workspaces && yarn run validate --workspaces",
    "validate:examples:type": "yarn run build --workspaces && yarn run validate:type --workspaces",
    "validate:examples:app": "yarn run build --workspaces && yarn run validate:app --workspaces",
    "clear": "yarn run clear:build && yarn run clear:types",
    "clear:build": "yarn run clear:build --workspace=@ts-for-gir/cli",
    "clear:types": "rimraf ./@types",
    "clear:types:gjs": "rimraf ./@types/Gjs",
    "clear:types:node": "rimraf ./@types/node-gtk",
    "watch": "yarn run build:ts --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sammydre/ts-for-gjs.git"
  },
  "author": "",
  "files": [
    "src",
    "bin",
    "lib",
    "templates"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sammydre/ts-for-gjs/issues"
  },
  "homepage": "https://github.com/sammydre/ts-for-gjs#readme",
  "devDependencies": {
    "ava": "^4.3.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.2",
    "typescript": "4.7.4"
  },
  "workspaces": [
    "examples/*/*",
    "packages/*"
  ],
  "packageManager": "yarn@3.2.1",
  "dependencies": {
    "@yarnpkg/pnpify": "^4.0.0-rc.11"
  },
  "dependenciesMeta": {
    "ts-node@10.8.2": {
      "unplugged": true
    }
  }
}
