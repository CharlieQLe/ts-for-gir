#!/usr/bin/env node
require('source-map-support').install()
const { Errors } = require('@oclif/core')
require('@oclif/core')
    .run()
    .then(require('@oclif/core/flush'))
    .catch((error) => {
        if (error && error.oclif) {
            Errors.handle(error)
        }
        console.error(error)
    })
