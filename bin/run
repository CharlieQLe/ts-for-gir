#!/usr/bin/env node
require('source-map-support').install()
const { Errors } = require('@oclif/core')
require('@oclif/core')
    .run()
    .then(require('@oclif/core/flush'))
    .catch((error) => {
        if (error && error.oclif && typeof error.oclif.exit === 'number' && error.oclif.exit !== 0) {
            console.log('error', error)
        }
        Errors.handle(error)
    })
